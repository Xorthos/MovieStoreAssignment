@model List<Proxy.DomainModels.Customer>

@{
    ViewBag.Title = "Customer";
}

<h2>Customer</h2>


    <table class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>First Name</th>
                        <th>Middle Name</th>
                        <th>Last Name</th>
                        <th>Street Name</th>
                        <th>Street Number</th>
                        <th>Email</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Id</td>
                            <td>@item.FirstName</td>
                            <td>@item.MiddleName</td>
                            <td>@item.LastName</td>
                            <td>@item.StreetName</td>
                            <td>@item.StreetNumber</td>
                            <td>@item.Email</td>
                        </tr>
                    }
                </tbody>
            </table>
         


 
